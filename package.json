{
  "name": "journal",
  "version": "1.0.0",
  "description": "Notifies NodeJs Rest API using TDD, Clean Architecture and Typescript",
  "main": "index.js",
  "author": "Anderson Pereira",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env NODE_ENV=test tsnd -r tsconfig-paths/register --inspect --transpile-only --respawn --ignore-watch node_modules src/main/server.ts",
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "lint": "eslint 'src/**' --fix",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "check": "npm-check -s -u",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "pretest": "yarn typeorm migration:run -c test",
    "test": "cross-env NODE_ENV=test jest --silent --noStackTrace --passWithNoTests --runInBand --forceExit",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.18",
    "@types/node": "^14.0.5",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "copyfiles": "^2.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-standard-with-typescript": "^16.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.3.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "mockdate": "^3.0.2",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^3.5.7",
    "nodemon": "^2.0.4",
    "pg": "^8.3.3",
    "sqlite3": "^5.0.0",
    "sucrase": "^3.15.0",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.28",
    "validator": "^13.0.0"
  },
  "engines": {
    "node": "12.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
