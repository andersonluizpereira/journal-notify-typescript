{
  "name": "journal",
  "version": "1.0.0",
  "description": "Notifies NodeJs Rest API using TDD, Clean Architecture and Typescript",
  "main": "index.js",
  "author": "Anderson Pereira",
  "license": "ISC",
  "scripts": {
    "sonar:scanner": "sonar-scanner",
    "build": "rimraf dist && babel src --extensions \".js,.ts\" --out-dir dist",
    "type": "module",
    "dev": "cross-env NODE_ENV=test tsnd -r tsconfig-paths/register --inspect --transpile-only --respawn --ignore-watch node_modules src/main/server.ts",
    "dev:start": "cross-env NODE_ENV=test tsnd -r tsconfig-paths/register --inspect --transpile-only --respawn --ignore-watch node_modules src/main/server.ts",
    "start": "node dist/main/server.js",
    "lint": "eslint 'src/**' --fix",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "check": "npm-check -s -u",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "pretest": "yarn typeorm migration:run -c test",
    "test": "cross-env NODE_ENV=test jest --silent --noStackTrace --passWithNoTests --runInBand --forceExit",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "rimraf clean_test_journal.sqlite && npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/ioredis": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/node": "^14.14.13",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "sonarqube-scanner": "^2.8.0",
    "sucrase": "^3.12.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "fast-glob": "^3.1.1",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.17",
    "npm-check": "^5.9.2",
    "pg": "^8.5.1",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.29",
    "validator": "^13.5.2"
  },
  "engines": {
    "node": "12.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
